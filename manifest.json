{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Streamsnip ",
  "version": "1.4",
  "description": "Shows clips from the stream on scrubber.",
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "ss.js"
      ],
      "css": [
        "ss.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
